html
  head
    title Front-End Capstone Presentation
    meta(charset='utf-8')
    script(src='http://html5slides.googlecode.com/svn/trunk/slides.js')
  style.
    .slides > article.slide1 {
      background: url('images/group-biking-sillouette-optimized.jpg');
    }
    .slides > article.slide1 h1 {
      color: white;
      margin-top: 12px;
    }
    .slides > article.slide1 p {
      color: white
    }
    .slides > article.slide2 {
      background-color: rgb(180, 255, 0);
      //- background-color: rgb(255, 235, 0);
    }
    .slides > article.slide2 li, .slides > article.slide2 ul {
      color: #222;
    }
    .slides > article.slide3 {
      background-color: rgb(0, 173, 255);
    }
    .slides > article.slide4 {
      background-color: rgb(255, 0, 129);
    }
    .slides > article.slide5 {
      background-color: rgb(122, 0, 255);
    }
    .slides > article.slide5 p, .slides > article.slide4 li, .slides > article a, .slides > article a:visited {
     color: white;
    }
  body(style='display: none')
    section.slides.layout-regular.template-default
